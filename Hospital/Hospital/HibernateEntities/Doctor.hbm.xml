<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	namespace="DAOLayer" assembly="DAOLayer">
	<class name="Doctor" table="Doctor">
		<id name="ID_Doctor" column="ID_Doctor">
			<generator class="native"/>
		</id>
		<property name="Lastname" column="Lastname"  />
		<property name="Firstname" column="Firstname"  />
		<property name="FieldOfMedicine" column="FieldOfMedicine"  />
		<bag name="OrderOfPatientInList" inverse="true">
			<key column="ID_Doctor"/>
			<one-to-many class="OrderOfPatient" />
		</bag>
	</class>
</hibernate-mapping>

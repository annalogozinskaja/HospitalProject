<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DAOLayer" assembly="DAOLayer">
	<class name="Speciment" table="Speciment">
		<id name="ID_Speciment" column="ID_Speciment">
			<generator class="native"/>
		</id>
		<property name="SpecimentName" column="SpecimentName"  />
		<bag name="SpecimentsInOrderList" inverse="true">
			<key column="ID_Speciment"/>
			<one-to-many class="SpecimentsInOrder" />
		</bag>
	</class>
</hibernate-mapping>

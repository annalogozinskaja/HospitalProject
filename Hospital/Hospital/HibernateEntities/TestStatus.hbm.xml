<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Hospital" assembly="Hospital">
	<class name="TestStatus" table="TestStatus">
		<id name="ID_TestStatus" column="ID_TestStatus">
			<generator class="native"/>
		</id>
		<property name="TestStatusName" column="TestStatusName"  />
		<bag name="TestsInOrderList" inverse="true">
			<key column="ID_TestStatus"/>
			<one-to-many class="TestsInOrder" />
		</bag>
	</class>
</hibernate-mapping>

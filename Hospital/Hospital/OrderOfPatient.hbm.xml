<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	namespace="Hospital" assembly="Hospital">
	<class name="OrderOfPatient" table="OrderOfPatient">
		<id name="ID_Order" column="ID_Order">
			<generator class="native"/>
		</id>
		<property name="DateOrder" column="DateOrder"  />
		<property name="Symptoms" column="Symptoms"  />
		<many-to-one name = "OrderStatus" class="OrderStatus" column = "ID_OrderStatus"  lazy="false" cascade = "save-update"/>
		<bag name="PatientInList" table="DoctorPatientRelations" inverse="true" lazy="false" cascade = "save-update">
			<key column="ID_Order"/>
			<many-to-many class="Patient" column="ID_Patient"/>
		</bag>
		<bag name="DoctorInList" table="DoctorPatientRelations" lazy="false" cascade = "save-update">
			<key column="ID_Order"/>
			<many-to-many class="Doctor" column="ID_Doctor"/>
		</bag>
		<bag name="SpecimentsInOrderList" inverse="true">
			<key column="ID_Order"/>
			<one-to-many class="SpecimentsInOrder" />
		</bag>
	</class>
</hibernate-mapping>

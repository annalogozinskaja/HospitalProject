<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	namespace="Hospital" assembly="Hospital">
	<class name="SpecimentsInOrder" table="SpecimentsInOrder">
		<id name="ID_SpecimentOrder" column="ID_SpecimentOrder">
			<generator class="native"/>
		</id>
		<property name="ID_SpecimentStatus" column="ID_SpecimentStatus"  />
		<property name="DateOfTaking" column="DateOfTaking"  />
		<property name="Nurse" column="Nurse"  />
		<many-to-one name = "OrderOfPatient" class="OrderOfPatient" column = "ID_Order" cascade = "save-update"/>
		<many-to-one name = "Speciment" class="Speciment" column = "ID_Speciment" cascade = "save-update"/>
	</class>
</hibernate-mapping>

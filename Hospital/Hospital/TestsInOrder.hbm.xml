<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	namespace="Hospital" assembly="Hospital">
	<class name="TestsInOrder" table="TestsInOrder">
		<id name="ID_TestOrder" column="ID_TestOrder">
			<generator class="native"/>
		</id>
		<property name="DateStart" column="DateStart"  />
		<property name="DateEnd" column="DateEnd"  />
		<property name="ID_TestStatus" column="ID_TestStatus"  />
		<property name="Result" column="Result"  />
		<many-to-one name = "Test" class="Test" column = "ID_Test" cascade = "save-update"/>
		<bag name="SpecimentsInOrder" table="TestsOfSpeciment" inverse="true" lazy="false" cascade = "save-update">
			<key column="ID_TestOrder"/>
			<many-to-many class="SpecimentsInOrder" column="ID_SpecimentOrder"/>
		</bag>
	</class>
</hibernate-mapping>

